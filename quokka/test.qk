class Car(colour) {
    speed = 10; // Any initialisation goes here

    def print_colour {
        >> colour;
    }

    def set_colour(col) {
        // Use the set keyword to assign the variable to the objet
        set colour = col; 
    }
}

car = Car("red");

car.print_colour; // red

car.set_colour("blue");

car.print_colour; // blue


